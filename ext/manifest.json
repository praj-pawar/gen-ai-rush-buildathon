{
    "manifest_version": 3,
    "name": "TalkTube",
    "version": "3.0",
    "permissions":["activeTab", "declarativeContent"],
    "action": {
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "http://localhost:5000/*",
      "https://localhost:5000/*"
    ],
    "icons": {
        "16": "logo.png",
        "32": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    }
  }
  